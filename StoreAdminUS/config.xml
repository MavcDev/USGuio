<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.storeadminus349819" version="0.0.1"
	xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
	<name>StoreAdminUS</name>
	<description>
		An Ionic Framework and Cordova project.
	</description>
	<author email="hi@ionicframework" href="http://ionicframework.com/">
		Ionic Framework Team
	</author>
	<content src="index.html" />
	<access origin="*" />
	<preference name="webviewbounce" value="false" />
	<preference name="UIWebViewBounce" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="BackupWebStorage" value="none" />
	<feature name="StatusBar">
		<param name="ios-package" value="CDVStatusBar" onload="true" />
	</feature>
	<feature name="SQLitePlugin">
		<param name="android-package" value="org.pgsqlite.SQLitePlugin" />
		<param name="ios-package" value="SQLitePlugin" />
		<param name="wp-package" value="SQLitePlugin" />
	</feature>
	<feature name="Brodysoft SQLitePlugin">
		<param name="id" value="com.brodysoft.sqlitePlugin" />
	</feature>
	<feature name="Keyboard">
		<param name="android-package" value="com.ionic.keyboard.IonicKeyboard" />
		<param name="onload" value="true" />
		<param name="ios-package" value="IonicKeyboard" />
		<param name="id" value="com.ionic.keyboard" />
	</feature>
	<feature name="BarcodeScanner">
		<param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
		<param name="ios-package" value="CDVBarcodeScanner" />
		<param name="wp-package" value="BarcodeScanner" />
		<param name="id" value="com.phonegap.plugins.barcodescanner" />
	</feature>
	<feature name="Console">
		<param name="ios-package" value="CDVLogger" />
		<param name="wp-package" value="DebugConsole" />
		<param name="id" value="org.apache.cordova.console" />
	</feature>
	<feature name="Device">
		<param name="android-package" value="org.apache.cordova.device.Device" />
		<param name="ios-package" value="CDVDevice" />
		<param name="wp-package" value="Device" />
		<param name="id" value="org.apache.cordova.device" />
	</feature>
</widget>